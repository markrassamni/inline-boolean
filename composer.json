{
    "name": "markrassamni/inline-boolean",
    "description": "A Laravel Nova boolean field that can be directly edited from the index or detail page of a resource.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MarkRassamni\\InlineBoolean\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mark Rassamni",
            "email": "markrassamni@me.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MarkRassamni\\InlineBoolean\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}