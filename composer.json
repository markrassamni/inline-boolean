{
    "name": "markrassamni/inline-boolean",
    "description": "A Laravel Nova boolean field that can be directly edited from the index or detail page of a resource.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/markrassamni/inline-boolean",
    "type": "library",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "MarkRassamni\\InlineBoolean\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Rassamni",
            "email": "markrassamni@me.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MarkRassamni\\InlineBoolean\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
